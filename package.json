    "envio:build": "cd indexer && npm run build",
    "envio:deploy": "cd indexer && npm run deploy",
    "setup": "npm install && npm run compile",
    "deploy:all": "npm run deploy:sepolia && npm run seed:sepolia && npm run envio:codegen && npm run envio:build",
    "verify": "hardhat verify --network sepolia"
